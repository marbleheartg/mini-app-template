{
  "name": "mini-app-template",
  "module": "index.ts",
  "private": true,
  "scripts": {
    "fork": "clear && bunx hardhat node --fork https://mainnet.optimism.io",
    "test": "clear && bun run clean && bun run compile && bun run deploy && bun wagmi generate && bunx hardhat test --network localhost",
    "clean": "clear && bunx hardhat clean",
    "compile": "clear && bunx hardhat compile",
    "deploy": "clear && bunx hardhat ignition deploy ./ignition/modules/Contract.ts --network localhost --reset",
    "tunnel": "clear && ngrok http 8545"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox-viem": "^5.0.0",
    "@nomicfoundation/hardhat-viem": "^3.0.0",
    "@types/bun": "latest",
    "dotenv-cli": "^10.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "@types/mocha": "^10.0.10",
    "dotenv": "^17.2.3",
    "hardhat": "^3.0.9",
    "viem": "^2.38.4"
  }
}
